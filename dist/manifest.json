{
  "manifest_version": 2,
  "name": "GibJob",
  "version": "0.1.0",
	"background": {
		"scripts": ["popup.js"],
		"persistent": true
	},
  "permissions": ["<all_urls>",
        "tabs",
        "http://*/",
        "https://*/"
	],
	"browser_action": {
		"default_icon": "icon-34.png",
		"default_popup": "popup.html"
	},
  "icons": {
    "128": "img/icon-128.png",
    "34": "img/icon-34.png"
  }
}
